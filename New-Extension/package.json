{
	"name": "pyre-vscode",
	"version": "0.0.2",
	"publisher": "fb-pyre-check",
	"engines": {
		"vscode": "^1.15.0"
	},
	"license": "MIT",
	"displayName": "pyre-vscode",
	"description": "VSCode extension for Pyre-check",
	"keywords": [
		"python",
		"pyre",
		"typechecking"
	],
	"main": "./out/main",
	"activationEvents": [
		"workspaceContains:**/.pyre_configuration"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"lint": "tslint --force -p ."
	},
	"dependencies": {
		"@vscode/python-extension": "^1.0.5",
		"vscode-languageclient": "^3.5.0",
		"which": "^4.0.0"
	},
	"devDependencies": {
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@types/mocha": "^2.2.44",
		"@types/which": "^3.0.4",
		"cson": "^4.1.0",
		"tslint": "^5.8.0",
		"tslint-microsoft-contrib": "^5.2.1",
		"typescript": "^5.5.4"
	}
}
