{
	"name": "react-webview-extension",
	"version": "1.0.0",
	"description": "A VS Code extension with React Webview",
	"main": "dist/extension.js",
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode development",
		"watch": "webpack --mode development --watch",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "reactWebview.start",
				"title": "Open React Webview"
			}
		]
	},
	"engines": {
		"vscode": "^1.85.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/vscode": "^1.85.0",
		"css-loader": "^6.7.3",
		"html-webpack-plugin": "^5.5.0",
		"postcss": "^8.4.21",
		"postcss-loader": "^7.1.0",
		"style-loader": "^3.3.2",
		"tailwindcss": "^3.3.1",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.5",
		"webpack": "^5.76.1",
		"webpack-cli": "^5.0.1"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
