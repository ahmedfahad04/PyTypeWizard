{
	"name": "pytypewizard",
	"displayName": "PyTypeWizard",
	"description": "Python Static Type Error Resolver",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.96.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pytypewizard.openDashboard",
				"title": "Open PyTypeWizard Dashboard"
			},
			{
				"command": "pytypewizard.refresh",
				"title": "Refresh PyTypeWizard"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "pytypewizard-sidebar",
					"title": "PyTypeWizard Sidebar",
					"icon": "media/magic_wand.svg"
				}
			]
		},
		"views": {
			"pytypewizard-sidebar": [
				{
					"type": "webview",
					"id": "pytypewizard-sidebar",
					"name": "Dashboard",
					"icon": "asset/magic_wand.svg",
					"contextualTitle": "Dashboard"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-typescript": "^8.1.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/polka": "^0.5.1",
		"@types/vscode": "^1.52.0",
		"concurrently": "^5.3.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^8.0.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.31.0",
		"svelte-check": "^1.1.23",
		"svelte-preprocess": "^4.6.1",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	}
}
