{
	"name": "pytypewizard",
	"displayName": "PyTypeWizard",
	"description": "Python Static Type Error Resolver",
	"publisher": "CodeGenBD",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.96.0"
	},
	"categories": [
		"Programming Languages",
		"AI",
		"Linters"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ahmedfahad04/SPL3"
	},
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "PyTypeWizard",
				"command": "pytypewizard.openDashboard",
				"title": "Open Dashboard"
			},
			{
				"category": "PyTypeWizard",
				"command": "pytypewizard.refresh",
				"title": "Refresh PyTypeWizard"
			},
			{
				"category": "PyTypeWizard",
				"command": "pytypewizard.showHistory",
				"title": "Show History",
				"icon": "$(history)"
			},
			{
				"category": "PyTypeWizard",
				"command": "pytypewizard.openSettings",
				"title": "Open Settings",
				"icon": "$(gear)"
			},
			{
				"category": "PyTypeWizard",
				"command": "pytypewizard.indexProject",
				"title": "Index Project"
			},
			{
				"category": "PyTypeWizard",
				"command": "pytypewizard.searchCode",
				"title": "Search Code"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "pytypewizard-sidebar",
					"title": "PyTypeWizard Sidebar",
					"icon": "media/magic_wand.svg"
				}
			]
		},
		"views": {
			"pytypewizard-sidebar": [
				{
					"type": "webview",
					"id": "pytypewizard-sidebar",
					"name": "Dashboard",
					"icon": "asset/magic_wand.svg",
					"contextualTitle": "Dashboard"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "pytypewizard.showHistory",
					"when": "view == pytypewizard-sidebar",
					"group": "navigation",
					"icon": "$(history)"
				},
				{
					"command": "pytypewizard.openSettings",
					"when": "view == pytypewizard-sidebar",
					"group": "navigation",
					"icon": "$(gear)"
				}
			]
		},
		"configuration": {
			"title": "PyTypeWizard Settings",
			"properties": {
				"pytypewizard.enabledErrorTypes": {
					"type": "array",
					"default": [
						"Incompatible variable type",
						"Incompatible parameter type",
						"Incompatible return type",
						"Invalid type",
						"Unbound name",
						"Incompatible attribute type"
					],
					"description": "Select the error types to enable in PyTypeWizard diagnostics.",
					"items": {
						"type": "string",
						"enum": [
							"Incompatible variable type",
							"Incompatible parameter type",
							"Incompatible return type",
							"Invalid type",
							"Unbound name",
							"Incompatible attribute type",
							"Unsupported operand",
							"Call error"
						]
					}
				},
				"pytypewizard.geminiApiKey": {
					"type": "string",
					"default": "",
					"description": "API Key for Google Gemini"
				},
				"pytypewizard.enableCodeLens": {
					"type": "boolean",
					"default": true,
					"description": "Enable/disable the PyTypeWizard CodeLens feature"
				},
				"pytypewizard.llmProvider": {
					"type": "string",
					"enum": [
						"gemini",
						"openai",
						"olama"
					],
					"default": "gemini",
					"description": "Select the LLM provider to use"
				},
				"pytypewizard.ollamaEndpoint": {
					"type": "string",
					"default": "http://localhost:11434",
					"description": "Ollama API endpoint"
				},
				"pytypewizard.ollamaModel": {
					"type": "string",
					"default": "codellama",
					"description": "Ollama model to use (e.g., codellama, llama2, mistral)"
				},
				"pytypewizard.openaiApiKey": {
					"type": "string",
					"default": "",
					"description": "OpenAI API Key"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "tsc -p ./ && webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch\" \"tsc -watch -p ./\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"dependencies": {
		"@google/generative-ai": "^0.21.0",
		"@vscode-elements/elements": "^1.9.0",
		"@vscode/python-extension": "^1.0.5",
		"@vscode/vsce": "^3.2.1",
		"axios": "^1.7.9",
		"better-sqlite3": "^11.7.2",
		"highlight.js": "^11.11.1",
		"markdown": "^0.5.0",
		"markdown-it": "^14.1.0",
		"marked": "^15.0.6",
		"openai": "^4.80.1",
		"rimraf": "^6.0.1",
		"sqlite3": "^5.1.7",
		"uuid": "^11.0.5",
		"vscode-languageclient": "^9.0.1",
		"vscode-languageserver-protocol": "^3.17.5",
		"web-tree-sitter": "^0.24.6",
		"which": "^4.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-typescript": "^8.1.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/polka": "^0.5.1",
		"@types/vscode": "^1.52.0",
		"@types/which": "^3.0.4",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"concurrently": "^5.3.0",
		"copy-webpack-plugin": "^12.0.2",
		"eslint": "^8.0.0",
		"glob": "^7.1.6",
		"minimatch": "^10.0.1",
		"mocha": "^8.1.3",
		"node-loader": "^2.1.0",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"supports-color": "^10.0.0",
		"svelte": "^3.31.0",
		"svelte-check": "^1.1.23",
		"svelte-preprocess": "^4.6.1",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"files": [
		"out/**/*",
		"media/**/*",
		"gui/**/*"
	]
}
